    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-redux-saga": "1.0.0",
    "lint-staged": "^11.0.0",
    "pre-commit": "1.2.2",
    "prettier": "1.17.0"
